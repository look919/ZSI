<Application x:Class="ZSI.Projekt.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ZSI.Projekt"
             xmlns:viewModels="clr-namespace:ZSI.Projekt.ViewModels"
             xmlns:views="clr-namespace:ZSI.Projekt.Views"
             StartupUri="Views\MainWindow.xaml">
    <Application.Resources>

        <DataTemplate DataType="{x:Type viewModels:MainViewModel}">
            <views:MainView/>
        </DataTemplate>

        <DataTemplate DataType="{x:Type viewModels:AddTrainingVectorViewModel}">
            <views:AddTrainingVectorView/>
        </DataTemplate>

    </Application.Resources>
</Application>
